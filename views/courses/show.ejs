<div class="container">
    <h2 class="center">Course Data for <%= course.title %></h2>
    <table class="table">
      <tbody>
        <tr>
          <td>Title</td>
          <td><%= course.title %></td>
        </tr>
        <tr>
          <td>Description</td>
          <td><%= course.description %></td>
        </tr>
        <tr>
          <td>Cost</td>
          <td>$<%= course.cost %></td>
        </tr>
        <tr>
          <td>Max students</td>
          <td><%= course.maxStudents %></td>
        </tr>
      </tbody>
    </table>
  </div>
  